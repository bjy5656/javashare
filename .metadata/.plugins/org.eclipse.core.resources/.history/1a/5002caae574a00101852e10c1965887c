package variable;

public class Variable02 { //클래스 중괄호 영역 시작 
	public static void main(String[] args) { //main  메소드 중괄호영역 시작 
		//여러개의 변수를 선언하기 
		int num1 = 10; 
		int num2 = 20; 
		int num3 = 30; 
		System.out.println(num1);
		System.out.println(num2);
		System.out.println(num3);
		System.out.println(num1+num2+num3);
		
	} //main 메소드 중괄호 영역 끝 
} //클래스 중괄호 영역 끝 
